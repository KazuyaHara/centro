{
  "name": "web",
  "description": "The place where you were.",
  "version": "0.0.1",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.9.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "firebase": "^9.9.0",
    "framer-motion": "^6.5.1",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^11.18.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "unstated-next": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "main": "src/index.tsx",
  "private": true,
  "scripts": {
    "build:dev": "cp .env.development .env.production.local && react-scripts build",
    "build:prd": "cp .env.production .env.production.local && react-scripts build",
    "build:stg": "cp .env.staging .env.production.local && react-scripts build",
    "deploy:dev": "yarn build:dev && firebase use default && firebase deploy --only hosting",
    "deploy:prd": "yarn build:prd && firebase use production && firebase deploy --only hosting && firebase use default",
    "deploy:stg": "yarn build:stg && firebase use staging && firebase deploy --only hosting && firebase use default",
    "eject": "react-scripts eject",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "start": "react-scripts start"
  }
}
